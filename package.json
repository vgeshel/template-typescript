{
  "name": "template-typescript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production bun run --bun next build",
    "start": "bun run --bun next start",
    "lint": "bunx --bun eslint",
    "typecheck": "bunx --bun tsc --noEmit",
    "test": "bunx vitest --no-file-parallelism",
    "test:ui": "bunx vitest --ui --no-file-parallelism",
    "test:run": "bunx vitest run --no-file-parallelism",
    "test:coverage": "bunx vitest run --coverage --no-file-parallelism",
    "format": "bunx prettier --write .",
    "prepare": "husky"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.52.0",
    "@tanstack/react-query": "^5.90.19",
    "@trpc/client": "^11.8.1",
    "@trpc/next": "^11.8.1",
    "@trpc/react-query": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "@types/pino": "^7.0.5",
    "commander": "^14.0.3",
    "next": "16.1.6",
    "pino": "^10.2.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "superjson": "^2.2.6",
    "zod": "^4.3.6"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mjs}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.3.6",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-istanbul": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.18"
  }
}
